{  
    "manifest_version": 2,  
    "name": "WBCJ",  
    "version": "1.0",  
    "description": "wu Bin & Chen Jian",  
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"background":
    {
        "scripts": ["background.js"],
		"permissions": [
		"tabs",  "notifications"
		]
    },
    "browser_action": {  
		"default_icon": "favicon.ico",
		"default_title": "Wu Bin & Chen Jian",
        "default_popup": "popup.html" ,
		"permissions": [
		"tabs",  "notifications"
		]
    }
}  